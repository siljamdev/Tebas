            <WordsStyle name="LINE COMMENTS" fgColor="7F9F7F" bgColor="FFFFFF" colorStyle="1" fontStyle="2" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="C969D3" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
